{
  "github": {
    "owner": "dug-21",
    "repo": "devtemplate",
    "pollInterval": 300000,
    "enabled": true,
    "projectNumber": null,
    "projectColumnId": null
  },
  "phases": {
    "idea": {
      "agents": ["researcher", "analyst"],
      "topology": "mesh",
      "strategy": "research",
      "autoAdvance": false,
      "nextPhase": "research"
    },
    "research": {
      "agents": ["researcher", "analyst", "coordinator"],
      "topology": "hierarchical",
      "strategy": "analysis",
      "autoAdvance": true,
      "nextPhase": "planning"
    },
    "planning": {
      "agents": ["analyst", "coder", "coordinator"],
      "topology": "hierarchical",
      "strategy": "planning",
      "autoAdvance": false,
      "nextPhase": "implementation"
    },
    "implementation": {
      "agents": ["coder", "tester", "coordinator"],
      "topology": "star",
      "strategy": "development",
      "autoAdvance": false,
      "nextPhase": "testing"
    }
  },
  "automation": {
    "welcomeNewIssues": true,
    "autoLabel": true,
    "postUpdates": true,
    "triggerSwarms": true,
    "processingDelay": 2000
  },
  "filtering": {
    "ignoreDrafts": true,
    "ignoreLabels": ["automation:ignore", "no-automation", "wip"],
    "requireLabels": [],
    "processOnlyPhases": [],
    "completionLabel": "swarm-processed",
    "autoCloseLabel": "auto-close-on-complete",
    "keepOpenLabel": "keep-open"
  },
  "templates": {
    "welcome": "üëã Thanks for opening this issue! Our enhanced GitHub workflow automation will process this with a dedicated swarm.",
    "swarmActivated": "üêù A dedicated {phase} swarm has been deployed to analyze this issue.",
    "analysisComplete": "‚úÖ Swarm analysis complete. See comprehensive results above.",
    "error": "‚ùå An error occurred during processing. The team has been notified."
  }
}